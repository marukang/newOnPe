<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"

    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="316"
        android:layout_weight="380"
        android:background="@color/white"
        >

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />


        <TextView
            android:id="@+id/result_score_time_exercise_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="40"
            android:textColor="#404040"
            android:text="순서"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />



        <TextView
            android:id="@+id/result_score_time_exercise_category_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="40"
            android:textColor="#404040"
            android:text="종목"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />

        <TextView
            android:id="@+id/result_score_time_exercise_title_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="68"
            android:textColor="#404040"
            android:text="대분류"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />

        <TextView
            android:id="@+id/result_score_time_exercise_name_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="60"
            android:textColor="#404040"
            android:text="동작명"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />


        <TextView
            android:id="@+id/result_score_average_score_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="64"
            android:textColor="#404040"
            android:text="평균점수"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:visibility="gone"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />

        <TextView
            android:id="@+id/result_score_time_count_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="36"
            android:textColor="#404040"
            android:text="횟수"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />

        <TextView
            android:id="@+id/result_score_time_textview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="64"
            android:textColor="#404040"
            android:text="이용시간(초)"
            android:fontFamily="@font/notosanscjkkr_medium"
            android:includeFontPadding="false"
            android:textSize="11dp"
            android:textStyle="normal"
            android:gravity="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            />
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#f1f1f1"
            />

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f1f1f1"
        />
</LinearLayout>